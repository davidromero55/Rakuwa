<h1>Hello World!!</h1><small>[% date %]</small><div class="row">    <div class="col-6">        <h3>Controller</h3>        <ul>            [% for item in controller %]            <li>[% item.key %] = [% item.value %]</li>            [% end %]        </ul>        <h3>Params</h3>        <ul>            [% for item in params %]            <li>[% item.key %] = [% item.value %]</li>            [% end %]        </ul>        <h3>Response headers</h3>        <ul>            [% for item in headers %]            <li>[% item.key %] = [% item.value %]</li>            [% end %]        </ul>    </div>    <div class="col-6">        <h3>ENV</h3>        [% for item in env %]        <div class="row">            <div class="col-6"><small>[% item.key %]</small></div>            <div class="col-6"><small>[% item.value %]</small></div>        </div>        [% end %]    </div></div>
